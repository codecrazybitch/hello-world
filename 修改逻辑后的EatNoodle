class Noodle
{
	  private String length;
	  private String kind;
	  Noodle(String length,String kind)
	  {
		  this.length = length;
		  this.kind = kind;
		  System.out.println(length);
		  System.out.println(kind);
	  }
}	
class Person
{
	static void Eat()
	{
		for(int i = 0; i < 5;i++)
		{
			System.out.println("one by one");
		}
	}
	public static int Eat(int bowl)
	{
			if(bowl > 3)
			{
				System.out.println("Are your a pig?");
			}else{
				System.out.println("I'm full");
			}
			return 0;
	}
}
class  EatNoodle
{
	public static void main(String[] args) 
	{
		Noodle noodle = new Noodle("5","ningxia");
		System.out.println(new Person());
		Person.Eat();
		Person.Eat(1);
	}
}
